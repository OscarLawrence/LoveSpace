#!/usr/bin/env python3
"""
Axiom CLI launcher script
Usage: ./axiom [command] [args...]
"""

import sys
import os
from pathlib import Path

# Add the axiom source to Python path
axiom_path = Path(__file__).parent / "projects" / "axiom" / "src"
sys.path.insert(0, str(axiom_path))

# Import and run the CLI
from axiom.cli import main

if __name__ == "__main__":
    main()